{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "forOf": true,
        "jsx": true,
        "es6": true
    },
    "rules": {
        "comma-dangle": 0,
        "new-cap": 0,
        "prefer-const": 1,
        "indent": [2, 2, {"SwitchCase": 1}],
        "react/prop-types": 0,
        "react/jsx-closing-bracket-location": [1, "after-props"]
    }
}
